<?xml version="1.0"?>
<ArrayOfSessionPattern xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SessionPattern>
    <SessionPatternString>(c_user=.+?)\.\.</SessionPatternString>
    <SessionName>Facebook</SessionName>
    <HTTPHost>.*?\.facebook\.com</HTTPHost>
    <Webpage>https://www.facebook.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>JSESSIONID=.+</SessionPatternString>
    <SessionName>LinkedIn</SessionName>
    <HTTPHost>.*?\.linkedin\.com</HTTPHost>
    <Webpage>http://www.linkedin.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>guest_id=.+?</SessionPatternString>
    <SessionName>Wordpress</SessionName>
    <HTTPHost>.*?\.wordpress\.com</HTTPHost>
    <Webpage>http://www.wordpress.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>_twitter_sess=</SessionPatternString>
    <SessionName>Twitter</SessionName>
    <HTTPHost>.*twitter\.com</HTTPHost>
    <Webpage>https://twitter.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>navcmd=</SessionPatternString>
    <SessionName>Paypal</SessionName>
    <HTTPHost>www\.paypal\.com</HTTPHost>
    <Webpage>https://www.paypal.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>GMAIL_LOGIN=</SessionPatternString>
    <SessionName>gMail</SessionName>
    <HTTPHost>mail\.google\.com</HTTPHost>
    <Webpage>https://mail.google.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>cid=[^;]+;</SessionPatternString>
    <SessionName>ebay</SessionName>
    <HTTPHost>.*?\.ebay\.com</HTTPHost>
    <Webpage>http://www.ebay.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>_pinterest_sess=.+</SessionPatternString>
    <SessionName>Pinterest</SessionName>
    <HTTPHost>.*?pinterest\.com</HTTPHost>
    <Webpage>http://pinterest.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>PHPSESSID=.+</SessionPatternString>
    <SessionName>Hyves</SessionName>
    <HTTPHost>.*?hyves\.nl</HTTPHost>
    <Webpage>http://www.hyves.nl</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>PHPSESSID=.+</SessionPatternString>
    <SessionName>9GAG</SessionName>
    <HTTPHost>9gag\.com</HTTPHost>
    <Webpage>http://www.9gag.com</Webpage>
  </SessionPattern>
  <SessionPattern>
    <SessionPatternString>sessionid=.+?</SessionPatternString>
    <SessionName>Steam</SessionName>
    <HTTPHost>steampowered\.com</HTTPHost>
    <Webpage>http://store.steampowered.com/</Webpage>
  </SessionPattern>
</ArrayOfSessionPattern>